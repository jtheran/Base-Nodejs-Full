    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "os": "^0.1.2",
    "os-utils": "^0.0.14",
    "passport": "^0.7.0",
