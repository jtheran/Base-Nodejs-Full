{
  "name": "back-base-nodejs",
  "version": "1.0.0",
  "description": "Backend Base hecho en NodeJS con las mejores practicas de Calidad y Seguridad",
  "keywords": [
    "PRISMA",
    "NODEJS",
    "WINSTON",
    "CORS",
    "DOTENV",
    "PASSPORT",
    "JOI",
    "JWT"
  ],
  "license": "MIT",
  "author": "JMTB",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon src/app.js",
    "start": "node src/app.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "node src/seed/initial-data.js",
    "db:studio": "prisma studio",
    "test:jest": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "config:check": "node -e \"import('./src/scripts/check-config.js'); console.log('✅ Configuración cargada correctamente');\"",
    "config:db": "node -e \"import('./src/scripts/check-db.js'); console.log('✅ Configuración de DB cargada correctamente');\""
  },
  "dependencies": {
    "@prisma/adapter-better-sqlite3": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@prisma/instrumentation": "^7.0.1",
    "accesscontrol": "^2.2.1",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "bull-board": "^1.7.2",
    "check-disk-space": "^3.4.0",
    "colors": "^1.4.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "express-xss-sanitizer": "^2.0.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.8.2",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
